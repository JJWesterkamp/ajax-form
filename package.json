{
  "name": "@jjwesterkamp/ajax-form",
  "version": "0.1.0",
  "author": "Jeffrey Westerkamp",
  "repository": "https://github.com/JJWesterkamp/ajax-form",
  "description": "[In progress] Ajax / XHR form implementation for barebone web projects.",
  "keywords": [],
  "license": "MIT",
  "main": "./dits/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/laravel-ajax-form.types.d.ts",
  "scripts": {
    "dev": "run-p \"build:** -- --watch\"",
    "build": "npm run clean:dist && run-p build:**",
    "build:cjs": "tsc -m commonjs --outdir dist/cjs",
    "build:esm": "tsc -m es6 --outDir dist/esm",
    "build:umd": "webpack",
    "clean:dist": "rm -rf ./dist",
    "lint": "run-p lint:**",
    "lint:source": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "lint:declaration": "tslint -c tslint.json '*.d.ts' --fix",
    "prepublishOnly": "npm-run-all lint test:ci build",
    "test": "karma start karma.conf.js",
    "test:ci": "karma start --single-run --no-auto-watch karma.conf.js"
  },
  "dependencies": {
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.0",
    "@types/webpack": "^4.41.12",
    "@types/webpack-env": "^1.15.2",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^5.0.5",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-coveralls": "^2.1.0",
